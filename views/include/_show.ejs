<%for(var i=0; i<users.length; i++){%>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].bssid %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].number %></td>
    <!-- <td><%= users[i].gender %></td> -->
    <!-- <td><%= users[i].attendence =='P'?"Present":"Absent" %></td> -->
    <td>
        <a href="/update-user?id=<%= users[i]._id%>"class="button <%= users[i].attendence =='P'?"Present":"Absent" %>"><%= users[i].attendence =='P'?"Present":"Absent" %></a>
    </td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<%}%>